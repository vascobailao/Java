public class Calc {
	
	private FolhaCalc _folha;
	/**
	 * @param args
	 */
	public Calc(){
		_folha=new FolhaCalc;
	 
	public FolhaCalc getFolha(){
		return _folha;
	}
	
	public FolhaCalc setFolha(FolhaCalc folha){
			_folha=folha;
	}
	
	
	public static void main(String args[]) {
		FolhaCalc folha  = null;
		Calc app=new Calc();
		Menu m = new CalcMenu(app);
		m.open();
	}
